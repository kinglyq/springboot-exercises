<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="priv.lyq.springboot.datasource.mapper.MultipleTestMapper">

    <select id="selectMultipleTest" resultType="priv.lyq.springboot.datasource.entity.MultipleTest">
        SELECT *
        FROM multiple_test
    </select>

    <insert id="insertMultipleTest">
        INSERT INTO multiple_test(name)
        VALUES (#{name, jdbcType=VARCHAR})
    </insert>

</mapper>